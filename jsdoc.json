{
    "plugins": ["plugins/markdown", "jsdoc-http-plugin"],
    "source": {
        "include": ["src"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs|package.json)"
    },
    "templates":{
        "cleverLinks":false,
        "monospaceLinks":false
    },
    "opts": {
        "recurse": true,
        "destination": "./docs/",
        "template":"node_modules/docdash",
        "tutorials": "tutorials",
        "readme":"./README.md"
    }
}